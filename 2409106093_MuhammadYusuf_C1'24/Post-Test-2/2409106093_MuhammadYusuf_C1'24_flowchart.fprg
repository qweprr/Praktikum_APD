    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="gulaku, maniskita, gunungmadu" type="String" array="False" size=""/>
            <declare name="namalengkap, nim" type="String" array="False" size=""/>
            <output expression="&quot;NAMA LENGKAP&quot;" newline="True"/>
            <input variable="namalengkap"/>
            <output expression="&quot;NIM&quot;" newline="True"/>
            <input variable="nim"/>
            <declare name="harga, hargadiskongulaku, hargadiskonmaniskita, hargadiskongunungmadu" type="Real" array="False" size=""/>
            <output expression="&quot;harga&quot;" newline="True"/>
            <input variable="harga"/>
            <assign variable="hargadiskongulaku" expression="harga - (harga * 0.07)"/>
            <assign variable="hargadiskonmaniskita" expression="harga - (harga * 0.11)"/>
            <assign variable="hargadiskongunungmadu" expression="harga - (harga * 0.13)"/>
            <output expression="namalengkap" newline="True"/>
            <output expression="nim" newline="True"/>
            <output expression="&quot;gulaku: &quot;&amp; hargadiskongulaku" newline="True"/>
            <output expression="&quot;manis kita: &quot;&amp; hargadiskonmaniskita" newline="True"/>
            <output expression="&quot;gunung madu: &quot;&amp; hargadiskongunungmadu" newline="True"/>
        </body>
    </function>
</flowgorithm>
